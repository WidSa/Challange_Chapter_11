{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["next", "standard"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-unused-expressions": "off"
  }
}
